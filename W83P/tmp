using System;

class Program
{
    static void Main(string[] args)
    {
        // Create a timer that will trigger every 5 minutes
        Timer timer = new Timer(5 * 60 * 1000);

        // Define the function that will be called when the timer triggers
        timer.Elapsed += new ElapsedEventHandler(MyFunction);

        // Start the timer
        timer.Start();

        // Wait for the user to press a key
        Console.ReadKey();
    }

    static void MyFunction(object source, ElapsedEventArgs e)
    {
        // Code to be executed every 5 minutes goes here
        Console.WriteLine("Hello, I'm called every 5 minutes!");
    }
}
